{
  "preset": "ts-jest/presets/default-esm",
  "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
  "globals": {
    "ts-jest": {
      "useEsm": "true",
      "compiler": "typescript",
      "transformIgnorePatterns": [
        "node_modules/(?!@cloudinary)"
      ],
      "transform": {
        "^.+\\.(ts|tsx)?$": "ts-jest",
        "^.+\\.(js|jsx)$": "babel-jest"
      }
    }
  }
}
