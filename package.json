{
  "name": "folloze-sdk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/src/index.d.js",
  "typings": "dist/src/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "description": "",
  "scripts": {
    "lint": "node_modules/.bin/eslint **/*.ts",
    "lint:fix": "node_modules/.bin/eslint --fix **/*.ts",
    "dev": "npm-run-all --parallel build:es test:watch",
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "build": "npm run build:es",
    "build:es": "npm run lint:fix && rimraf dist && node ./esbuild.config.js && npm run ts-types",
    "dont:use:build:ts": "tsc --build",
    "dont:use:build:ts:watch": "tsc --build --watch",
    "test": "npm run lint:fix && jest --verbose",
    "test:watch": "npm run lint:fix && jest --watch --onlyChanged"
  },
  "author": "Rotem Grimberg <rotem@folloze.com>",
  "license": "",
  "dependencies": {
    "axios": "^0.21.4",
    "lit": "^2.0.2",
    "lodash": "^4.17.20",
    "object-hash": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.2.118",
    "@swc/jest": "^0.2.12",
    "@types/jest": "^27.0.3",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "axios-mock-adapter": "^1.20.0",
    "core-js": "^3.8.1",
    "esbuild": "^0.14.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.1.0",
    "jest": "^27.4.3",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.0",
    "ts-loader": "^5.4.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5",
    "web-types-lit": "^0.1.0"
  },
  "homepage": "https://github.com/folloze/client-sdk",
  "repository": "https://github.com/folloze/client-sdk",
  "files": [
    "/analytics.{d.ts.map,d.ts,js.map,js}",
    "/analytics/",
    "/liveboard.{d.ts.map,d.ts,js.map,js}",
    "/liveboard/",
    "/common.{d.ts.map,d.ts,js.map,js}",
    "/common/",
    "/types.{d.ts.map,d.ts,js.map,js}",
    "/common/interfaces/",
    "/designer.{d.ts.map,d.ts,js.map,js}",
    "/designer/",
    "sdk.{d.ts.map,d.ts,js.map,js}",
    "index.{d.ts.map,d.ts,js.map,js}"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./sdk.js": "./dist/sdk.js",
    "./analytics.js": "./dist/analytics/analytics.js",
    "./liveboard.js": "./dist/liveboard/liveboard.js",
    "./common.js": "./dist/common/common.js",
    "./types.js": "./dist/common/interfaces/types.js",
    "./designer.js": "./dist/designer/designer.js"
  },
  "web-types": "./node_modules/web-types-lit/web-types.json"
}
